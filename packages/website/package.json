{
  "name": "website",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "postcss styles/**/*.css --base styles --dir app/styles",
    "dev": "concurrently -c magenta,cyan \"npm:dev:*\"",
    "dev:remix": "node -r dotenv/config ./node_modules/@remix-run/dev/cli.js",
    "dev:css": "npm run build:css -- --w",
    "postinstall": "remix setup node",
    "start": "cross-env NODE_ENV=production remix-serve build"
  },
  "dependencies": {
    "@prisma/client": "^3.10.0",
    "@remix-run/react": "^1.2.3",
    "@remix-run/serve": "^1.2.3",
    "cross-env": "^7.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.2.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.2.3",
    "@remix-run/eslint-config": "^1.2.3",
    "@tailwindcss/typography": "^0.5.2",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "eslint": "^8.9.0",
    "postcss": "^8.4.8",
    "postcss-cli": "^9.1.0",
    "prisma": "^3.10.0",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14"
  }
}
